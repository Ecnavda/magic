{% extends "layout" %}

{% block main %}
    <span>Currently displaying all cards in the database.</span>
    {% if cards %}
        <ul>
            {% for card in cards %}
                <li>{{ card }}</li>
            {% endfor %}
        </ul>
    {% else %}
        <span>Card inventory belonging to the logged in profile goes here.</span>
    {% endif %}
{% endblock main %}